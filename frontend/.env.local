# Development (default) - will be used when VITE_ENV is not set or is 'development'
VITE_API_URL=http://localhost:3000
VITE_WEBSOCKET_URL=ws://localhost:3000

# Production - will be used when VITE_ENV=production
# These values will be overridden by the actual production environment variables
VITE_API_URL_PRODUCTION=https://devconnect-project.onrender.com
VITE_WEBSOCKET_URL_PRODUCTION=wss://devconnect-project.onrender.com